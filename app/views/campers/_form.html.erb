<% if flash[:errors] %>
  <ul>
    <% flash[:errors].each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>

<%=form_for @camper do |f|  %>
<%= f.label "Name: " %><br><br>
<%= f.text_field :name %><br><br>
<%= f.label "Age: " %><br><br>
<%= f.text_field :age %><br><br>
<%= f.submit %><br><br>
<% end%>